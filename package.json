{
  "name": "@dworznik/typescript-starter",
  "version": "0.0.5",
  "description": "TypeScript project template",
  "engines": {
    "node": ">= 10 <=16"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf coverage dist tmp",
    "build": "ttsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src __tests__ --ext .ts",
    "postinstall": "patch-package"
  },
  "keywords": [
    "typescript",
    "starter"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "author": "Patryk DwÃ³rznik",
  "license": "MIT",
  "homepage": "https://github.com/dworznik/typescript-starter#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/dworznik/typescript-starter.git"
  },
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.40",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-prettier": "^4.0.0",
    "fetch-mock-jest": "^1.5.1",
    "husky": "4",
    "jest": "^28.1.1",
    "jest-extended": "^2.0.0",
    "lint-staged": "^13.0.0",
    "patch-package": "^6.4.7",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "tsutils": "^3.21.0",
    "ttypescript": "^1.5.13",
    "typescript": "4.7.3"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5",
    "date-fns": "^2.28.0"
  }
}
